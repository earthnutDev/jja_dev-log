{
  "name": "@qqi/dev-log",
  "type": "module",
  "version": "0.2.0",
  "private": true,
  "description": "在 dev 环境使用 name_dev 启动日志打印",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "jja rm dist && npm run b && npm run clean:package",
    "clean:package": "node scripts/clean-package-json.js",
    "diff": "jja pkg --diff",
    "eslint": "jja cls && eslint src",
    "dev": "jja rm .eg && rollup --config rollup.dev.config.js && node .eg/index.mjs",
    "beautify": "npm run prettier",
    "prettier": "jja cls && prettier . --write",
    "test": "jja cls && lint-staged && jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "push:version": "gvv",
    "push:submit": "gvv",
    "vjj": "vjj",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "a-js-tools": ">=0.6.0 <1.0.0",
    "a-type-of-js": ">=0.2.0 <1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "a-node-tools": "^2.2.2",
    "codecov": "^3.8.3",
    "color-pen": "^1.3.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsdoc": "^50.6.11",
    "globals": "^16.0.0",
    "gvv": "^0.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jja": "^2.2.3",
    "lint-staged": "^15.5.1",
    "pjj": "^0.2.3",
    "prettier": "^3.5.3",
    "rollup": "^4.40.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vjj": "^0.3.2"
  }
}
